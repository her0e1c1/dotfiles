[user]
	name = her0e1c1
	email = her0e1c1@gmail.com
[alias]
	co = checkout
	ci = commit
	s = status -uno
	st = status
	rt = remote
	au = add -u
	com = checkout master

    # diff
    d = diff
    dc = "diff --cached"

    # log
	l = log -n 5
	lp = log -p
    ld = 'log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short'

    # branch
	b = branch -av
    bm = branch --merged
    bn = branch --no-merged

[color]
	ui = true
[core]
	editor = vim
	excludesfile = ~/.gitignore
[push]
    default = matching
