_complie_and_run_in_c(){
    local main source tfile
    main=$1; shift;
    source="
$__C_INCLUDED_HEADERS
extern char **environ;
int main(int argc, char* argv[]){
    $main
    return 0;
}"
    tfile=`mktemp`
    echo $source | clang -x c - -o $tfile && $tfile "$@"
    [ -f $tfile ] && \rm $tfile
}

cl (){
	clang++ -I. $1 -std=c++11 && ./a.out
}